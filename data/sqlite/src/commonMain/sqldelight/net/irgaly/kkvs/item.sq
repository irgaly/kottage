CREATE TABLE item (
  -- "{type}+{key}"
  key TEXT PRIMARY KEY,
  type TEXT NOT NULL,
  value TEXT,
  created_at INTEGER NOT NULL,
  last_read INTEGER NOT NULL,
  expire_at INTEGER NOT NULL
);
CREATE INDEX item_type ON item(type);
CREATE INDEX item_created_at ON item(created_at);
CREATE INDEX item_last_read ON item(last_read);
CREATE INDEX item_expire_at ON item(expire_at);

selectItem:
SELECT * FROM item WHERE key = ?;

deleteItem:
DELETE FROM item WHERE key = ?;
